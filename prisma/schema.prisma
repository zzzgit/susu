generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

enum Gender {
    F
    M
}

model customer {
    id        Int      @id @default(autoincrement())
    name      String
    gender    Gender?
    phone     String?
    extra     String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

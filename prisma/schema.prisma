generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model customer {
    id        String    @id @default(dbgenerated("uuidv7()")) @db.Uuid
    name      String
    gender    Gender?
    phone     String?
    extra     String?
    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    deletedAt DateTime?
}

enum Gender {
    F
    M
}
